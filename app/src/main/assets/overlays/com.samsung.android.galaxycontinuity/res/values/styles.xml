<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="AppTheme" parent="@*com.samsung.android.galaxycontinuity:style/Theme.SeslCompat.Light">
        <item name="android:textViewStyle">@*com.samsung.android.galaxycontinuity:style/RobotoTextViewStyle</item>
        <item name="android:textSelectHandleLeft">@*com.samsung.android.galaxycontinuity:drawable/editor_text_select_handle_left</item>
        <item name="android:textSelectHandleRight">@*com.samsung.android.galaxycontinuity:drawable/editor_text_select_handle_right</item>
        <item name="android:textSelectHandle">@*com.samsung.android.galaxycontinuity:drawable/editor_text_select_handle_middle</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.galaxycontinuity:style/myclose</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="Base.Widget.SeslCompat.Light.ActionBar" parent="@*android:style/Widget.DeviceDefault.ActionBar">
        <item name="android:background">@*com.samsung.android.galaxycontinuity:drawable/sesl_action_bar_background</item>
        <item name="android:divider">?android:dividerVertical</item>
        <item name="android:height">?android:actionBarSize</item>
        <item name="android:displayOptions">showTitle</item>
        <item name="android:actionButtonStyle">@*com.samsung.android.galaxycontinuity:style/Widget.SeslCompat.Light.ActionButton</item>
        <item name="android:actionOverflowButtonStyle">@*com.samsung.android.galaxycontinuity:style/Widget.SeslCompat.Light.ActionButton.Overflow</item>
        <item name="android:titleTextStyle">@*com.samsung.android.galaxycontinuity:style/TextAppearance.SeslCompat.Widget.ActionBar.Title.Inverse</item>
        <item name="android:subtitleTextStyle">@*com.samsung.android.galaxycontinuity:style/TextAppearance.SeslCompat.Widget.ActionBar.Subtitle.Inverse</item>
        <item name="android:backgroundStacked">@*com.samsung.android.galaxycontinuity:drawable/sesl_action_bar_background_stacked</item>
        <item name="android:backgroundSplit">@*com.samsung.android.galaxycontinuity:drawable/sesl_action_bar_background_split</item>
        <item name="android:elevation">@*com.samsung.android.galaxycontinuity:dimen/sesl_action_bar_elevation</item>
        <item name="android:contentInsetStart">@*com.samsung.android.galaxycontinuity:dimen/sesl_action_bar_content_inset</item>
        <item name="android:contentInsetEnd">0.0dip</item>
        <item name="android:popupTheme">@null</item>
    </style>
	    <style name="Platform.SeslCompat.Light" parent="@*android:style/Theme.Material.NoActionBar">
        <item name="android:colorBackground">@*com.samsung.android.galaxycontinuity:color/sesl_background_color_light</item>
        <item name="android:backgroundDimAmount">0.6</item>
        <item name="android:disabledAlpha">@*com.samsung.android.galaxycontinuity:dimen/sesl_disabled_color_alpha</item>
        <item name="android:textAppearance">@*com.samsung.android.galaxycontinuity:style/TextAppearance.SeslCompat</item>
        <item name="android:textAppearanceInverse">@*com.samsung.android.galaxycontinuity:style/TextAppearance.SeslCompat.Inverse</item>
        <item name="android:textColorPrimary">@*com.samsung.android.galaxycontinuity:color/sesl_primary_text_light</item>
        <item name="android:textColorSecondary">@*com.samsung.android.galaxycontinuity:color/sesl_secondary_text_light</item>
        <item name="android:textColorPrimaryInverse">@*com.samsung.android.galaxycontinuity:color/sesl_primary_text_dark</item>
        <item name="android:textColorSecondaryInverse">@*com.samsung.android.galaxycontinuity:color/sesl_secondary_text_dark</item>
        <item name="android:textAppearanceLarge">@*com.samsung.android.galaxycontinuity:style/TextAppearance.SeslCompat.Large</item>
        <item name="android:textAppearanceMedium">@*com.samsung.android.galaxycontinuity:style/TextAppearance.SeslCompat.Medium</item>
        <item name="android:textAppearanceSmall">@*com.samsung.android.galaxycontinuity:style/TextAppearance.SeslCompat.Small</item>
        <item name="android:textAppearanceLargeInverse">@*com.samsung.android.galaxycontinuity:style/TextAppearance.SeslCompat.Large.Inverse</item>
        <item name="android:textAppearanceMediumInverse">@*com.samsung.android.galaxycontinuity:style/TextAppearance.SeslCompat.Medium.Inverse</item>
        <item name="android:textAppearanceSmallInverse">@*com.samsung.android.galaxycontinuity:style/TextAppearance.SeslCompat.Small.Inverse</item>
        <item name="android:windowBackground">@*com.samsung.android.galaxycontinuity:drawable/sesl_screen_background_light</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:textColorHighlight">@*com.samsung.android.galaxycontinuity:color/sesl_highlighted_text_color_light</item>
        <item name="android:listChoiceIndicatorSingle">@*com.samsung.android.galaxycontinuity:drawable/sesl_list_choice_btn_radio</item>
        <item name="android:listChoiceIndicatorMultiple">@*com.samsung.android.galaxycontinuity:drawable/sesl_list_choice_btn_check</item>
        <item name="android:colorBackgroundCacheHint">@*com.samsung.android.galaxycontinuity:color/sesl_background_cache_hint_selector_light</item>
        <item name="android:textSelectHandleLeft">@*com.samsung.android.galaxycontinuity:drawable/sesl_text_select_handle_left_light</item>
        <item name="android:textSelectHandleRight">@*com.samsung.android.galaxycontinuity:drawable/sesl_text_select_handle_right_light</item>
        <item name="android:textSelectHandle">@*com.samsung.android.galaxycontinuity:drawable/sesl_text_select_handle_middle_light</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:statusBarColor">@*com.samsung.android.galaxycontinuity:color/sesl_status_bar_color</item>
    </style>
    <style name="NonSeslAppTheme" parent="@*android:style/Theme.Material">
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="CommonDialogTheme" parent="@*android:style/Theme.Material.Dialog">
        <item name="android:windowMinWidthMajor">@*android:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@*android:dimen/dialog_min_width_minor</item>
    </style>
	<style name="RobotoTextViewStyle" parent="@*android:style/Widget.TextView">
        <item name="android:fontFamily">sans-serif</item>
    </style>
	<style name="AppTabTextAppearance" parent="@*com.samsung.android.galaxycontinuity:style/TextAppearance.Design.Tab">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">#ff000000</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:fontFamily">sans-serif-medium</item>
        <item name="*com.samsung.android.galaxycontinuity:textAllCaps">true</item>
    </style>
	<style name="Base.TextAppearance.SeslCompat.Widget.ActionBar.Subtitle.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Subtitle.Inverse">
        <item name="android:textSize">@*com.samsung.android.galaxycontinuity:dimen/sesl_action_bar_text_size_subtitle</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*com.samsung.android.galaxycontinuity:color/sesl_action_bar_text_color_subtitle</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
	<style name="Base.TextAppearance.SeslCompat.Widget.ActionBar.Title.Inverse" parent="@*android:style/TextAppearance.Material.Widget.ActionBar.Title.Inverse">
        <item name="android:textSize">@*com.samsung.android.galaxycontinuity:dimen/sesl_action_bar_text_size_title</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*com.samsung.android.galaxycontinuity:color/sesl_action_bar_text_color_title</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
	<style name="TabSelectedTextStyle">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textStyle">normal</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">0.0dip</item>
        <item name="android:textAllCaps">true</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
	<style name="TextViewDesc">
        <item name="android:textSize">@*com.samsung.android.galaxycontinuity:dimen/text_desc</item>
        <item name="android:textColor">@*com.samsung.android.galaxycontinuity:color/text_desc</item>
        <item name="android:lineSpacingExtra">4.0dip</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextViewDescLink">
        <item name="android:textSize">@*com.samsung.android.galaxycontinuity:dimen/text_desc</item>
        <item name="android:textColor">@*com.samsung.android.galaxycontinuity:color/text_desc_link</item>
        <item name="android:background">@*com.samsung.android.galaxycontinuity:drawable/ripple_round_rect</item>
        <item name="android:padding">@*com.samsung.android.galaxycontinuity:dimen/text_desc_link_padding</item>
        <item name="android:focusable">true</item>
        <item name="android:lineSpacingExtra">@*com.samsung.android.galaxycontinuity:dimen/text_desc_spacing</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextViewDescSmall">
        <item name="android:textSize">@*com.samsung.android.galaxycontinuity:dimen/text_desc</item>
        <item name="android:textColor">@*com.samsung.android.galaxycontinuity:color/text_desc_small</item>
        <item name="android:focusable">true</item>
        <item name="android:lineSpacingExtra">@*com.samsung.android.galaxycontinuity:dimen/text_desc_spacing</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextViewDescVersion">
        <item name="android:textSize">@*com.samsung.android.galaxycontinuity:dimen/text_desc_version</item>
        <item name="android:textColor">@*com.samsung.android.galaxycontinuity:color/text_desc_version</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextViewListItemDesc">
        <item name="android:textSize">@*com.samsung.android.galaxycontinuity:dimen/text_list_item_desc</item>
        <item name="android:textColor">@*com.samsung.android.galaxycontinuity:color/text_list_item_desc</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextViewListItemTitle">
        <item name="android:textSize">@*com.samsung.android.galaxycontinuity:dimen/text_list_item_title</item>
        <item name="android:textColor">@*com.samsung.android.galaxycontinuity:color/text_list_item_title</item>
        <item name="android:maxLines">1</item>
        <item name="android:fontFamily">sans-serif</item>
    </style>
    <style name="TextViewSubAppBar">
        <item name="android:textSize">@*com.samsung.android.galaxycontinuity:dimen/text_sub_app_bar</item>
        <item name="android:textColor">@*com.samsung.android.galaxycontinuity:color/text_sub_app_bar</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="TextViewTitle">
        <item name="android:textSize">@*com.samsung.android.galaxycontinuity:dimen/text_title</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*com.samsung.android.galaxycontinuity:color/text_title</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:minHeight">@*com.samsung.android.galaxycontinuity:dimen/text_title_height</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
	<style name="ToolbarTitleStyle">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">@*com.samsung.android.galaxycontinuity:color/colorToolBarTitle</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:padding">0.0dip</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
</resources>
