<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="AppBaseTheme" parent="@*android:style/Theme.DeviceDefault" />
	<style name="Theme.SettingsBase" parent="@*android:style/Theme.DeviceDefault">
		<item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="Theme.ActionBar" parent="@*android:style/Widget.DeviceDefault.ActionBar">
        <item name="android:displayOptions">showTitle</item>
        <item name="android:contentInsetStart">@*com.android.settings:dimen/actionbar_contentInsetStart</item>
    </style>
	<style name="Theme.NoActionBar" parent="@*android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="Theme.NoActionBar.RestrictedProfile" parent="@*com.android.settings:style/Theme.Settings">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
    <style name="Theme.ScreenModeActionBar" parent="@*com.android.settings:style/Theme.ActionBar">
        <item name="android:displayOptions">showTitle</item>
		<item name="android:backgroundTint">@null</item>
		<item name="android:background">@*android:drawable/status_bar_closed_default_background</item>
        <item name="android:contentInsetStart">0.0dip</item>
    </style>
	<style name="BiometricsLockSetupStyle" parent="@*android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="Theme.ApnSettings" parent="@android:style/Theme.DeviceDefault">
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.android.settings:preferenceTheme">@*com.android.settings:style/PreferenceTheme</item>
    </style>
	<style name="Theme.DialogWhenLarge" parent="@*com.android.settings:style/Theme.Settings">
        <item name="android:colorBackground">@*android:color/white</item>
        <item name="android:actionBarStyle">@*com.android.settings:style/Theme.ActionBar</item>
        <item name="android:statusBarColor">@*com.android.settings:color/status_bar_color</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.android.settings:preferenceBackgroundColor">@*com.android.settings:drawable/preference_background</item>
    </style>
	<style name="Theme.Settings.VzwSetupWizardWifi" parent="@*com.android.settings:style/Theme.Settings">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
        <item name="*com.android.settings:wifi_signal">@*com.android.settings:drawable/vzw_wifi_signal</item>
    </style>
	<style name="Theme.Translucent.NoTitleBar.LightStatusBar" parent="@*android:style/Theme.Translucent.NoTitleBar">
        <item name="android:windowDrawsSystemBarBackgrounds">true</item>
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="Theme_Bixby" parent="@*android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:statusBarColor">@*android:color/transparent</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="ProvisioningStyle" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
	<style name="FingerprintConfirmStyle" parent="@*android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>
	<style name="FaceTranslucentStyle" parent="@*android:style/Theme.DeviceDefault.NoActionBar.TranslucentDecor">
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowDisablePreview">true</item>
    </style>
	<style name="SettingsTheme" parent="@*android:style/Theme.DeviceDefault">
        <item name="*com.android.settings:list_item_check_box_margin_left">@*com.android.settings:dimen/list_item_checkbox_margin_left</item>
        <item name="*com.android.settings:list_item_check_box_margin_right">@*com.android.settings:dimen/list_item_checkbox_margin_right</item>
        <item name="*com.android.settings:list_item_check_box_width_for_delete_mode_animation">38.0dip</item>
    </style>
    <style name="SettingsTheme.DialogWhenLarge" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:actionModeCloseButtonStyle">@*com.android.settings:style/NoCloseButton</item>
    </style>
</resources>
