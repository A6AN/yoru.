        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.messaging:style/ActionModeCloseButton</item>
        <item name="*com.samsung.android.messaging:windowActionBar">true</item>
        <item name="*com.samsung.android.messaging:windowNoTitle">false</item>
    </style>
