        <item name="android:textAppearanceSmallInverse">@*com.samsung.android.messaging:style/TextAppearance.SeslCompat.Small.Inverse</item>
        <item name="android:windowBackground">@*com.samsung.android.messaging:drawable/sesl_screen_background_light</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:textColorHighlight">@*com.samsung.android.messaging:color/sesl_highlighted_text_color_light</item>
