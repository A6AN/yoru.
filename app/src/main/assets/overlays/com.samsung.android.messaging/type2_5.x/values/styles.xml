        <item name="android:windowMinWidthMajor">@*android:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@*android:dimen/dialog_min_width_minor</item>
        <item name="android:colorPrimary">@*com.samsung.android.messaging:color/color_primary</item>
        <item name="android:colorPrimaryDark">@*com.samsung.android.messaging:color/msg_color_primary_dark</item>
