        <item name="android:textColorPrimaryInverse">@*com.samsung.android.messaging:color/sesl_primary_text_dark</item>
        <item name="android:textColorSecondaryInverse">@*com.samsung.android.messaging:color/sesl_secondary_text_dark</item>
        <item name="android:textAppearanceLarge">@*com.samsung.android.messaging:style/TextAppearance.SeslCompat.Large</item>
        <item name="android:textAppearanceMedium">@*com.samsung.android.messaging:style/TextAppearance.SeslCompat.Medium</item>
        <item name="android:textAppearanceSmall">@*com.samsung.android.messaging:style/TextAppearance.SeslCompat.Small</item>
