<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="BottomSheet" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:backgroundDimAmount">0.4</item>
        <item name="android:windowBackground">@*android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@*android:style/Animation</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowTranslucentStatus">true</item>
    </style>
	 <style name="ClockComponentButton.Custom" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:colorControlActivated">@*com.samsung.android.scloud:color/o100_48bccd_no_theme</item>
    </style>
    <style name="CustomAlertDialogTheme" parent="@*android:style/Theme.DeviceDefault.Dialog">
        <item name="android:windowMinWidthMajor">@*android:dimen/dialog_min_width_major</item>
        <item name="android:windowMinWidthMinor">@*android:dimen/dialog_min_width_minor</item>
        <item name="android:colorPrimaryDark">@*com.samsung.android.scloud:color/o100_ed920f_no_theme</item>
    </style>
	<style name="DriveTheme" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/NoCloseButton</item>
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="GalleryActionBar" parent="@*android:style/Widget.DeviceDefault.ActionBar">
        <item name="android:background">@*com.samsung.android.scloud:color/o100_fafafa_no_theme</item>
        <item name="android:actionModeCloseDrawable">@null</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/HideActionModeClose</item>
        <item name="android:titleTextStyle">@*com.samsung.android.scloud:style/GalleryActionBar.Title</item>
        <item name="android:contentInsetStart">0.0dip</item>
    </style>
	<style name="Theme.BNRActionMode" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/actionmode_no_close_button</item>
        <item name="android:colorControlActivated">@*android:color/material_deep_teal_500</item>
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="Theme.BillingActionBar" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:backgroundDimAmount">@*android:color/transparent</item>
    </style>
	<style name="Theme.BillingTheme" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionBarStyle">@*com.samsung.android.scloud:style/Theme.BillingActionBar</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/actionmode_no_close_button</item>
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="Theme.DashBoardBurstShotDetail" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowBackground">@*android:color/black</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/actionmode_no_close_button</item>
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="Theme.SCloudTheme" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionButtonStyle">@*com.samsung.android.scloud:style/OverFlowButtonStyle</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/actionmode_no_close_button</item>
        <item name="android:colorControlActivated">@*android:color/material_deep_teal_500</item>
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="Theme.SettingPreference" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionBarStyle">@*com.samsung.android.scloud:style/GalleryActionBar</item>
        <item name="android:alertDialogTheme">@*com.samsung.android.scloud:style/CustomAlertDialogTheme</item>
        <item name="android:actionMenuTextColor">@*com.samsung.android.scloud:color/o100_ed920f_no_theme</item>
        <item name="android:colorControlActivated">@*com.samsung.android.scloud:color/o100_ed920f_no_theme</item>
        <item name="android:colorPrimary">@*com.samsung.android.scloud:color/o100_ed920f_no_theme</item>
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="Theme.Setupwizard" parent="@*android:style/Theme.DeviceDefault.NoActionBar">
        <item name="android:windowDisablePreview">true</item>
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="UploadSheet" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="android:actionModeCloseButtonStyle">@*com.samsung.android.scloud:style/NoCloseButton</item>
        <item name="android:statusBarColor">#ff000000</item>
        <item name="android:windowLightStatusBar">false</item>
    </style>
	<style name="OverFlowButtonStyle" parent="@*android:style/Widget.Material.ActionButton">
        <item name="android:backgroundDimAmount">@*android:color/transparent</item>
        <item name="android:paddingLeft">@*com.samsung.android.scloud:dimen/abc_action_bar_overflow_padding_end_material</item>
        <item name="android:paddingRight">@*com.samsung.android.scloud:dimen/abc_action_bar_overflow_padding_start_material</item>
        <item name="android:tint">@*android:color/material_deep_teal_500</item>
    </style>
	<style name="switchColorStyle">
        <item name="android:colorControlActivated">@*android:color/material_deep_teal_500</item>
    </style>
    <style name="CardViewBackgroundTheme">
        <item name="android:background">?*android:selectableItemBackground</item>
        <item name="android:foreground">@*com.samsung.android.scloud:drawable/cardview_background</item>
        <item name="android:radius">@*com.samsung.android.scloud:dimen/dash_board_card_view_radius</item>
        <item name="android:backgroundTint">#ff000000</item>
    </style>
</resources>
