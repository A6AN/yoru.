<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--QS related values-->
    <style name="Theme.DeviceDefault.QuickSettings" parent="@*android:style/Theme.DeviceDefault">
        <item name="android:colorControlNormal">#ffffffff</item>	<!--OG:?textColorPrimary-->
        <item name="android:colorPrimary">@*android:color/primary_material_dark</item>    <!--OG:@*android:color/primary_device_default_settings_light-->
