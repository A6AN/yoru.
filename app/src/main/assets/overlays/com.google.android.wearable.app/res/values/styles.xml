<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="AppBaseTheme.Light" parent="@*android:style/Theme.Material" />
	<style name="SBrowserTitleTextStyle" parent="@*android:style/TextAppearance.DeviceDefault.Widget.ActionBar.Title">
        <item name="android:textSize">17.0sp</item>
        <item name="android:textColor">@*android:color/material_deep_teal_500</item>
    </style>
	<style name="SettingsThemeForDialogBase" parent="@*android:style/Theme.Material.Dialog.MinWidth" />
	<style name="HelpIntroTheme" parent="@*android:style/Theme.Material">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:colorControlActivated">@*com.sec.android.app.sbrowser:color/color_control_activated</item>
        <item name="android:colorPrimary">@*com.sec.android.app.sbrowser:color/color_primary</item>
        <item name="android:colorPrimaryDark">@*com.sec.android.app.sbrowser:color/color_primary_dark</item>
        <item name="android:statusBarColor">@*com.sec.android.app.sbrowser:color/toolbar_statusbar_color</item>
    </style>
	<style name="SettingsThemeBase" parent="@*android:style/Theme.Material">
        <item name="android:windowNoTitle">false</item>
        <item name="android:spinnerItemStyle">@*com.sec.android.app.sbrowser:style/SettingsSpinnerItem</item>
        <item name="android:windowActionBar">true</item>
        <item name="android:actionBarStyle">@*com.sec.android.app.sbrowser:style/SBrowserActionBarStyle</item>
        <item name="android:actionButtonStyle">@*com.sec.android.app.sbrowser:style/SBrowserActionButtonStyle</item>
        <item name="android:actionOverflowButtonStyle">@*com.sec.android.app.sbrowser:style/SettingsOverflowButton</item>
        <item name="android:actionModeCloseButtonStyle">@*com.sec.android.app.sbrowser:style/CabModeActionButton</item>
        <item name="android:alertDialogTheme">@*com.sec.android.app.sbrowser:style/SettingsThemeForDialog</item>
        <item name="android:actionMenuTextColor">@*com.sec.android.app.sbrowser:color/settings_action_menu_text_color</item>
        <item name="android:actionModeStyle">@*com.sec.android.app.sbrowser:style/SBrowserActionModeStyle</item>
        <item name="android:colorControlActivated">@*com.sec.android.app.sbrowser:color/color_control_activated</item>
    </style>
</resources>
